{
  "id": "nginx",
  "cpus": 1.0,
  "mem": 512,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "nginx:1.9.7",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 80, "hostPort": {{port}}, "protocol": "tcp" }
      ]
    }
  },
  "healthChecks": [
    {
      "path": "/",
      "portIndex": 0,
      "protocol": "HTTP"
    }
  ]
}
